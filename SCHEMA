CREATE TABLE MEAL(
  PRICE DOUBLE,
  NAME VARCHAR(255),
  CALORIES INT
  );
 
INSERT INTO MEAL(NAME, PRICE, CALORIES)
VALUES('Spaghetti Bolognese', 16.00, 700),
      ('Grana sciolto su mele polacche', 3.00, 1000),
      ('Orribili pietanze olandesi', 9.99, 5000);


CREATE TABLE INGREDIENT(
  NAME VARCHAR(255),
  PRICE DOUBLE,
  CONTAINS_LACTOSE BOOLEAN,
  VEGETARIAN BOOLEAN,
  VEGAN BOOLEAN,
  GLUTEN_FREE BOOLEAN
  );
  
INSERT INTO INGREDIENT(NAME, PRICE, CONTAINS_LACTOSE, VEGETARIAN, VEGAN, GLUTEN_FREE)
VALUES('Eggs',2.99,FALSE,TRUE,FALSE,TRUE),
      ('Bread',3.99,FALSE,TRUE,TRUE,FALSE),
      ('Beef Patty',4.00,FALSE,FALSE,FALSE,TRUE),
      ('Lettuce',2.00,FALSE,TRUE,TRUE,TRUE),
      ('Yoghurt',2.00,TRUE,TRUE,FALSE,TRUE);
